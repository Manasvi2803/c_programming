#include <stdio.h>
int main(void)
{
    int t, n, sum = 0;
    scanf("%d", &t);

    for (int i = 0; i < t; ++i)
    {
        scanf("%d", &n);

        sum = 0;
        while (n != 0)
        {
            sum += n % 10;
            n = n / 10;
        }
        printf("%d\n", sum);
    }
    return 0;
}
